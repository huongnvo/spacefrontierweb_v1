        <table class="table table-hover">
            <thead>
                <tr>
                    <th> 
                        <a href="" ng-click="order('Band')">Band</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('Name')">Name</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('Loc')">Location</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('EIRP')">EIRP</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat ="part in parts | orderBy:predicate:reverse">
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Band}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Name}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Loc}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.EIRP}}</td>
                    <td><a type="button" ng-click="openEdit(part)" class="btn btn-warning">Edit</a></td>
                    <td><a type="button" class="btn btn-danger" ng-click="deletePart(part._id)">Delete</a> </td>
                </tr>
            </tbody>
        </table>
        <hr>
