<!DOCTYPE html>
<html data-ng-app="spaceFrontierApp">
<% include header %>

    <div class="container" data-ng-controller="searchController">
        <% include navbarLogin %>
        
        <h1>Subsystems List</h1>
        <hr></hr>
        <div class="row">
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="attitude()"  class="btn btn-block btn-default btn-lg btn-round">Attitude</a>
                </div>
            </div>
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="cdh()"  class="btn btn-block btn-default btn-lg btn-round">Command, Data & Handling</a>
                </div>
            </div>
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="comm()"  class="btn btn-block btn-default btn-lg btn-round">Communication</a>
                </div>
            </div>
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="instruments()"  class="btn btn-block btn-default btn-lg btn-round">Instruments</a>
                </div>
            </div>
        </div>
        <div class="divider"></div>
        <div class="row">
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="power()"  class="btn btn-block btn-default btn-lg btn-round">Power</a>
                </div>
            </div>
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="propulsion()"  class="btn btn-block btn-default btn-lg btn-round">Propulsion</a>
                </div>
            </div>
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="station()"  class="btn btn-block btn-default btn-lg btn-round">Ground Stations</a>
                </div>
            </div>
            <div class="col-lg-3">
                <div id="button">
                    <a type="button" ng-click="thermal()"  class="btn btn-default btn-block btn-lg btn-round">Thermal, Mechanical</a>
                </div>
            </div>
        </div>
        <hr>
        <% include footer %>
    </div>
</html>