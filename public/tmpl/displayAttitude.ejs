                    <div class="dividersmall"></div>
                    <div class="modal-data">
                        <form>
                            <div class="row">
                                <div class="col-md-1">
                                </div>
                                <div class="col-md-2"
                                    <label for="message-text" class="control-label">Datarate:</label>
                                </div>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" data-ng-model="datarate">
                                </div>
                                <div class="col-md-1">
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-default" ng-click="addPart()" data-dismiss="modal">Submit</button>
                    </div>
                </div>
            </div>
        </div>

        <h1>Attitude Subsystem</h1>
        <h4>Click on each line to learn more about the part</h4>
        <hr>

        <table class="table table-hover">
            <thead>
                <tr>
                    <th>
                        <a href="" ng-click="order('_id')">_id</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('Type')">Type</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('Name')">Name</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('Manufacturer')">Manufacturer</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat ="part in parts | orderBy:predicate:reverse">
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part._id}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Type}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Name}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Manufacturer}}</td>
                </tr>
            </tbody>
        </table>
        <hr>
        <div id="button">
            <a type="button" data-toggle="modal" data-target="#addNewPartModal" class="btn btn-primary">Add New Part</a>
            <a type="button" href="/list" class="btn btn-primary">Return to Main</a>
        </div>
        <hr>
        <% include footer %>
        <div class = "divider"></div>

    </body>
 
</html>