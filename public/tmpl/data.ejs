    <% include navbar %>
    <h1>Command and Data Handling Selection</h1>
    <p>Select which way you would like to calculate bits per second based on your current knowledge of specifications. Then, input how many millions of instructions per second your part must handle.</p>
    <hr>
    <h3>User Input</h3>
    <div class="row">
        <div class="col-lg-12">
            <div>
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#tab1">Channels</a></li>
                    <li>               <a href="#tab2">Arrays</a></li>
                    <li>               <a href="#tab3">Bits/Second</a></li>
                </ul>
            </div>
        </div>
    </div>

    <section id="tab1" class="tab-content active">
        <div class="dividersmall"></div>
        <div class="row">
            <div class="col-lg-6">
                Channels 
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="100" data-ng-model="channel" valid-number title="Suggested: ???-??? channels"/>
                    <span class="input-group-addon" id="Channels">Channels</span>
                </div>
                <div class="divider"></div>
                Bits/Channel: 
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="100" data-ng-model="bitschannel" valid-number title="Suggested: ??-?? bits/channel"/>
                    <span class="input-group-addon" id="Bits/Channel">Bits/Channel</span>
                </div>
                <div class="divider"></div>
                Integrations/Second:
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="100" data-ng-model="integrations1" valid-number title="Suggested: ??-?? integrations/second"/>
                    <span class="input-group-addon" id="Integrations/Second">Integrations/Second</span>
                </div>
                <div class="divider"></div>
                Percent of Time Generating Data:
                
                <div class="input-group">
                    <input class="form-control" placeholder="10" data-ng-model="resolution1" valid-number title="Suggested: ??-?? %"/>
                    <span class="input-group-addon" id="percent">%</span>
                </div>
            </div>
            <div class="col-lg-6">
            </div>
            <div class="col-lg-10">
            </div>
            <div class="col-lg-2">
                <div class = "dividerlarge"></div>
                    <div id="button">
                        <button type="button" class="btn btn-primary btn-block" id="round" ng-click="tabCHANNELS()" ng-disabled="channel==null||bitschannel==null||integrations1==null ||resolution1==null||channel.length==0||bitschannel==0||resolution1==0||integrations1.length==0" title="Show Bits/Second and Required Downlink">
                            Calculate
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <hr>
    </section>


    <section id="tab2" class="tab-content hide">
        <div class="dividersmall"></div>
        <div class="row">
            <div class="col-lg-6">
                Array Size - x: 

                <div class="input-group">
                    <input type="text" class="form-control" placeholder="100" data-ng-model="sizex" valid-number title="Suggested: ??-?? px"/>
                    <span class="input-group-addon" id="pixelsX"> pixels</span>
                </div>

                <div class="divider"></div>
                Array Size - y: 


                <div class="input-group">
                    <input type="text"class="form-control" placeholder="100" data-ng-model="sizey" valid-number title="Suggested: ??-?? px"/>
                    <span class="input-group-addon" id="pixelsY"> pixels</span>
                </div>

                <div class="divider"></div>
                Integrations/Second:

                <div class="input-group">
                     <input type="text" class="form-control" placeholder="100" data-ng-model="integrations2" valid-number title="Suggested: ??-?? integrations/second"/>
                    <span class="input-group-addon" id="pixelsY"> Integrations/Second</span>
                </div>

                <div class="divider"></div>                 
                Percent of Time Generating Data:
                <div class="input-group">
                    <input type="text" valid-number class="form-control" placeholder="10" data-ng-model="resolution2" title="Suggested: ??-?? %"/>
                    <div class="input-group-addon">%</div>
                </div>
            </div>
            <div class="col-lg-6">
            </div>
            <div class="col-lg-10">
            </div>
            <div class="col-lg-2">
                <div class = "dividerlarge"></div>
                    <div id="button">
                        <button type="button" class="btn btn-primary btn-block" id="round" ng-click="tabARRAY()" ng-disabled="sizex==null||sizey==null||integrations2==null||resolution2==null||sizex.length==0||sizey.length==0||integrations2.length==0||resolution2.length==0" title="Show Bits/Second and Required Downlink">
                            Calculate
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <hr>
    </section>

    <section id="tab3" class="tab-content hide">
        <div class="dividersmall"></div>
        <div class="row">
            <div class="col-lg-6">
                Bits/Second 

                <div class="input-group">
                     <input type="text" class="form-control" placeholder="100" data-ng-model="bitssecond" valid-number title="Suggested: ??-?? bits/second"/>
                    <div class="input-group-addon">Bits/Second</div>
                </div>

                <div class="divider"></div>
                Percent of Time Generating Data:
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="10" data-ng-model="resolution3" valid-number title="Suggested: ??-?? %"/>
                    <div class="input-group-addon">%</div>
                </div>
            </div>
            <div class="col-lg-6">
            </div>
            <div class="col-lg-10">
            </div>
            <div class="col-lg-2">
                <div class = "dividerlarge"></div>
                    <div id="button">
                        <button type="button" class="btn btn-primary btn-block" id="round" ng-click="tabBPS()" ng-disabled="bitssecond==null||resolution3==null||bitssecond.length==0||resolution3.length==0" title="Show Bits/Second and Required Downlink">
                            Calculate
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <hr>
    </section>

    <div ng-show="showCalculate">
        <h3>Calculation Results</h3>
        <div class="row">
            <div class="col-lg-6">
                <div class = "divider"></div>
                Bits/Second:

                <div class="input-group">
                    <input type="text" class="form-control" data-ng-model="totalbits" readonly="readonly" valid-number/>
                    <div class="input-group-addon">Bits/Second</div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class = "divider"></div>
                Required Downlink:

                <div class="input-group">
                    <input type="text" class="form-control" data-ng-model="totaldownlink" readonly="readonly"/>
                    <div class="input-group-addon">MIPS</div>
                </div>
            </div>
            <div class="col-lg-10">
            </div>
            <div class="col-lg-2">
                <div class = "dividerlarge"></div>
                    <div id="button">
                        <button type="button" class="btn btn-primary btn-block" id="round" ng-click="nextCDH()" title="Show Parts">
                            Select CDH
                        </button>
                    </div>
                </div>
            </div>
            <hr>
        </div>
    </div>

    <div ng-show="showDatabase">
        <div class="row">
            <div class="col-lg-6">
                <h3>Available CDH 
                    <div class="btn-group">
                        <button class="btn btn-default btn-xs dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Show:
                        <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" name="optionsRadios" id="optionsRadios1" value="option1" data-ng-model="OBC">
                                        Onboard Comp.
                                </label>
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" name="optionsRadios" id="optionsRadios1" value="option1" data-ng-model="bus">
                                        Bus
                                </label>
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" name="optionsRadios" id="optionsRadios1" value="option1" data-ng-model="imageprocessor">
                                        Image Processor
                                </label>
                            </div>
                        </ul>
                    </div>
                    <div class="btn-group">
                        <button class="btn btn-default btn-xs dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Sort By: 
                        <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <div class="radio">
                                <label>
                                    <input type="radio" name="optionsRadios" ng-model= "sort" value="Mass">
                                        Mass
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <input type="radio" name="optionsRadios" ng-model= "sort" value="Power">
                                        Power
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <input type="radio" name="optionsRadios" ng-model= "sort" value="Volume">
                                        Volume
                                </label>
                            </div>
                        </ul>
                    </div>
                </h3>
                <h6>Double click on the info button to learn more about a part, or on the right arrow to add it to the existing CubeSat.</h6>
                <div class="row">
                    <div class="col-lg-11">
                        <div class = "dividersmall"></div>
                        <select multiple class="form-control" size="16">
                            <option ng-click="savePart(part)" ng-repeat="part in cdhparts|filter:okType|filter:okData|orderBy:order">
                                {{part.Name}}
                            </option>
                        </select>
                    </div>
                    <div class="col-lg-1">
                        <a type="button" data-toggle="modal" data-target="#exampleModal" id="pushdown1" class="btn btn-sm btn-default" title="Info"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span></a>
                        <a type="button" ng-click="addPart()" id="pushdown2" class="btn btn-sm btn-default" title="Add Part"><span class="glyphicon glyphicon-menu-right" aria-hidden="true"></span></a>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <h3>Current Design</h3>
                <h6>Command & Data Handling Selection<br>Page 5/7</h6>
                <div class = "dividersmall"></div>
                <select multiple class="form-control" size="16">
                        <option>Name: {{Name}}</option>
                        <option>Objectives: {{Objectives}}</option>
                        <option>Target: {{target}}</option>
                        <option>Instrumentation: {{instrumentPart.Name}}</option>
                        <option>Attitude: {{attitudePart.Name}}</option>
                        <option>Propulsion: {{propulsionPart.Name}}</option>
                        <option>Ground Station: {{stationPart.Name}}</option>
                        <option>Antenna: {{antennaPart.Name}}</option>
                        <option>Receiver: {{receiverPart.Name}}</option>
                        <option>CDH: {{cdhPart.Name}}</option>
                        <option>Solar Panels: {{panelsPart.Name}}</option>
                        <option>Batteries: {{batteriesPart.Name}}</option>
                        <option>EPS: {{epsPart.Name}}</option>
                        <option>Bus: {{busPart.Name}}</option>
                        <option>Deployer: {{deployerPart.Name}}</option>
                        <option>Thermal: {{thermalPart.Name}}</option>
                </select>
            </div>
        </div>
        <div class="dividersmall"></div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class = "dividersmaller"></div>
