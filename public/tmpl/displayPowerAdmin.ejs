                        <div class="col-md-8">
                            <input type="text" class="form-control" data-ng-model="ener">
                        </div>
                        <div class="col-md-1">
                        </div>
                    </div>
                </form>
            </div>
            <div class="dividersmall"></div>
            <div class="modal-pper">
                <form>
                    <div class="row">
                        <div class="col-md-1">
                        </div>
                        <div class="col-md-2"
                            <label for="message-text" class="control-label">Panels/Wing:</label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" class="form-control" data-ng-model="pper">
                        </div>
                        <div class="col-md-1">
                        </div>
                    </div>
                </form>
            </div>
            <div class="dividersmall"></div>
            <div class="modal-wing">
                <form>
                    <div class="row">
                        <div class="col-md-1">
                        </div>
                        <div class="col-md-2"
                            <label for="message-text" class="control-label">Wings:</label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" class="form-control" data-ng-model="wings">
                        </div>
                        <div class="col-md-1">
                        </div>
                    </div>
                </form>
            </div>
            <hr>
            <div id="button">
                <button type="button" class="btn btn-default" ng-click="cancelEdit()">Cancel</button>
                <button type="button" class="btn btn-primary" ng-click="editPart()" data-dismiss="modal">Submit</button>
            </div>
            <hr>
        </div>

        <table class="table table-hover">
            <thead>
                <tr>
                    <th> 
                        <a href="" ng-click="order('Type')">Type</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('Name')">Name</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th> 
                        <a href="" ng-click="order('Manufacturer')">Manufacturer</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th>
                        <a href="" ng-click="order('Mass')">Mass</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th>
                        <a href="" ng-click="order('Power')">Power</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th>
                        <a href="" ng-click="order('Volume')">Volume</a>
                        <span class="sortorder" ng-show="predicate === 'name'" ng-class="{reverse:reverse}"></span>
                    </th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat ="part in parts | orderBy:predicate:reverse">
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Type}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Name}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Manufacturer}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Mass}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Power}}</td>
                    <td data-toggle="modal" data-target="#exampleModal" data-whatever={{part}}>{{part.Volume}}</td>
                    <td><a type="button" ng-click="openEdit(part)" class="btn btn-warning" ng-disabled="!admin">Edit</a></td>
                    <td><a type="button" class="btn btn-danger" ng-click="deletePart(part._id)" ng-disabled="!admin">Delete</a> </td>
                </tr>
            </tbody>
        </table>
        <hr>
        <div id="button">
            <a type="button" ng-click="backMain()" class="btn btn-default">Return to Main</a>
            <a type="button" data-toggle="modal" data-target="#addNewPartModal" class="btn btn-success">Add New Part</a>
        </div>
        <hr>
        <% include footer %>
        <div class = "divider"></div>

    </body>
 
</html>