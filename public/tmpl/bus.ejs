    <div ng-show="busPart.Mass == 'Unknown'">
        <div class="alert alert-danger" role="alert">We could not find an associated mass for this frame. We will be using an approximation of 500 g per U when calculating the final mass of the CubeSat.</div>
    </div>

    <div ng-show="busPart.Volume == 'Unknown'">
        <div class="alert alert-danger" role="alert">We could not find an associated volume for this frame. We will be using an approximation of 0.25 U when calculating the final volume of the CubeSat.</div>
    </div>

    <div ng-show="showBus">
        <div class="row">
            <div class="col-lg-6">
                <h3>Frame
                <div class="btn-group">
                    <button class="btn btn-default btn-xs dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    CubeSat Vol (U):
                    <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <li><input type="number" style="width:144px; margin-left:8px" class="form-control" placeholder="0 U" data-ng-model="volume" /></li>
                    </ul>
                </div>

                </h3>
                <h6>Click on the info button to learn more about a part, or on double click on the right arrow to add it to the existing CubeSat.</h6>
                <div class="row">
                    <div class="col-lg-11">
                        <div class = "dividersmall"></div>
                        <select multiple class="form-control" size="19">
                            <option ng-click="saveBus(part)" ng-repeat="part in parts|filter:busType">
                                {{part.Name}}
                            </option>
                        </select>       
                    </div>
                    <div class="col-lg-1">
                        <a type="button" data-toggle="modal" data-target="#busModal" id="pushdown1" class="btn btn-sm btn-default" title="Info"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span></a>
                        <a type="button" ng-click="addBus()" id="pushdown2" class="btn btn-sm btn-default" title="Add Part"><span class="glyphicon glyphicon-menu-right" aria-hidden="true"></span></a>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <h3>Current Design</h3>
                <h6>Bus Selection<br>Page 7/7</h6>
                <div class = "dividersmall"></div>
                <select multiple class="form-control" size="19">
                    <option>Name: {{Name}}</option>
                    <option>Objectives: {{Objectives}}</option>
                    <option>Target: {{target}}</option>
                    <option>Instrumentation: {{instrumentPart.Name}}</option>
                    <option>Attitude: {{attitudePart.Name}}</option>
                    <option>Propulsion: {{propulsionPart.Name}}</option>
                    <option>Ground Station: {{stationPart.Name}}</option>
                    <option>Antenna: {{antennaPart.Name}}</option>
                    <option>Receiver: {{receiverPart.Name}}</option>
                    <option>CDH: {{cdhPart.Name}}</option>
                    <option>Solar Panels: {{panelsPart.Name}}</option>
                    <option>Batteries: {{batteriesPart.Name}}</option>
                    <option>EPS: {{epsPart.Name}}</option>
                    <option>Bus: {{busPart.Name}}</option>
                    <option>Deployer: {{deployerPart.Name}}</option>
                    <option>Thermal: {{thermalPart.Name}}</option>
                </select>
            </div>
        </div>
        <div class = "dividersmall"></div>
        <div class = "row">
            <div class="col-lg-10">
            </div>
            <div class="col-lg-2">
